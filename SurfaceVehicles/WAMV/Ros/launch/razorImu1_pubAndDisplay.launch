<launch>
    <arg name="topicName" value="/razorImu1/data"/>
  
    <node pkg="razor_imu_9dof" type="imu_node.py" name="razorImu1" clear_params="true">
        <rosparam command="load" file="$(find razor_imu_9dof)/config/razorImu1.yaml"/>
        <remap from="/imu" to="$(arg topicName)"/>
    </node>
  
    <node pkg="razor_imu_9dof" type="display_3D_visualization.py" name="imuVisualization_razorImu1" output="screen" clear_params="true">
        <param name="topicName" value="$(arg topicName)"/>
    </node>
  
</launch>
